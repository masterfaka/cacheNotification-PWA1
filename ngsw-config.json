{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/assets/svg/*.gif"
        ],
        "urls": [
          "https://node-hnapi.herokuapp.com/news**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [{
    "name": "commentsJson",
    "urls": ["https://node-hnapi.herokuapp.com/item/**"],
    "cacheConfig": {
      "maxSize": 10,
      "maxAge": "12h",
      "timeout": "15s",
      "strategy": "freshness"
    }
  }
  ]
}
